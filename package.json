{
  "name": "StarMesh",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev:browser-extension": "pnpm --filter=browser-extension dev",
    "type-check:browser-extension": "pnpm --filter=browser-extension type-check",
    "dev:server": "pnpm --filter=mock-proxy-server start:dev",
    "type-check:server": "pnpm --filter=mock-proxy-server type-check",
    "type-check": "pnpm type-check:browser-extension && pnpm type-check:server",
    "format": "biome check --write --unsafe",
    "prepare": "husky"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "typescript": "catalog:"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "packageManager": "pnpm@10.17.0"
}
